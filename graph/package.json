    "remove-local": "graph remove --node http://localhost:8020/ gjeanmart/multisig",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 gjeanmart/multisig"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.18.0",
    "@graphprotocol/graph-ts": "0.18.1"
  },
  "devDependencies": {
    "mustache": "^3.1.0"
  }
}
